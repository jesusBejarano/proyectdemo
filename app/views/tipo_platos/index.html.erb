<h1>Listing tipo_platos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_platos.each do |tipo_plato| %>
      <tr>
        <td><%= tipo_plato.nombre %></td>
        <td><%= link_to 'Show', tipo_plato %></td>
        <td><%= link_to 'Edit', edit_tipo_plato_path(tipo_plato) %></td>
        <td><%= link_to 'Destroy', tipo_plato, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo plato', new_tipo_plato_path %>

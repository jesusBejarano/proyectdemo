<h1>Listing tipo_menus</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Preciofijo</th>
      <th>Precio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_menus.each do |tipo_menu| %>
      <tr>
        <td><%= tipo_menu.nombre %></td>
        <td><%= tipo_menu.PrecioFijo %></td>
        <td><%= tipo_menu.precio %></td>
        <td><%= link_to 'Show', tipo_menu %></td>
        <td><%= link_to 'Edit', edit_tipo_menu_path(tipo_menu) %></td>
        <td><%= link_to 'Destroy', tipo_menu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo menu', new_tipo_menu_path %>
